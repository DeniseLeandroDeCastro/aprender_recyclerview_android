<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ifrn.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_recycler_view"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_recycler_view"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecyclerView">
        <activity android:name="br.edu.ifrn.recyclerview.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>